--hd1 800 --hd2 800   --clusters 400  --gc_weight 0.1  --heads 4  --layers 1  --n_hop 3

--dataset dataset2 --task task3  --learn_rating 0.000005 --dr 0.5 --g_d 500 --batch_size 1024
